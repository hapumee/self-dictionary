{
  "name": "frame-react-and-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "reactstrap": "^8.1.1",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "start-react": "cross-env NODE_PATH=src react-scripts start",
    "build-react": "cross-env NODE_PATH=src react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-storybook": "start-storybook -p 9001 -c .storybook-app",
    "build-storybook": "build-storybook -c .storybook-app -o .storybook-app-dist",
    "deploy-storybook": "storybook-to-ghpages",
    "start-dev-server": "json-server -p 3001 src/data/words.list.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "cross-env": "^6.0.3"
  }
}
